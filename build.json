{
	"builds": [
		{
			"input": "../js-lib/yom",
			"output": "static/js/lib/yom"
		},
		{
			"input": "static/js/src/main-pkg.js",
			"output": "static/js/main-pkg.js"
		},
		{
			"input": "static/js/mod/src/article-list.js",
			"output": "static/js/mod/article-list.js"
		},
		{
			"input": "static/js/mod/src/read-article.js",
			"output": "static/js/mod/read-article.js"
		},
		{
			"input": "static/js/mod/src/write-article.js",
			"output": "static/js/mod/write-article.js"
		},
		{
			"input": "static/js/mod/src/about.js",
			"output": "static/js/mod/about.js"
		},
		{
			"input": "static/js/mod/src/about-profile.js",
			"output": "static/js/mod/about-profile.js"
		},
		{
			"input": "static/js/mod/src/about-contact.js",
			"output": "static/js/mod/about-contact.js"
		},
		{
			"input": "static/js/mod/src/about-work.js",
			"output": "static/js/mod/about-work.js"
		}
	],
	"combines": [
		{
			"inputs": ["../js-lib/yom/require-plugin/jsonp.js"],
			"output": "static/js/lib/yom/require-plugin/jsonp.js"
		},
		{
			"inputs": [
				"static/js/src/require-config.js",
				"../js-lib/yom/require/require.js"
			],
			"output": "static/js/require.js"
		},
		{
			"inputs": ["static/css/src/main.css", "static/css/src/util.css"],
			"output": "static/css/c.css"
		},
		{
			"inputs": ["static/css/src/form.css", "static/css/src/button.css"],
			"output": "static/css/form.css"
		},
		{
			"inputs": ["static/css/src/yuiEditor.css"],
			"output": "static/css/yuiEditor.css"
		}
	]
}