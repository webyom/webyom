	"combines": [
		{
			"inputs": ["../js-lib/yom/src/require/require.js"],
			"output": "static/js/lib/yom/require.js"
		},
		{
			"inputs": ["../js-lib/yom/require-plugin/src/jsonp/jsonp.js"],
