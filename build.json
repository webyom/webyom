{
	"builds": [
		{
			"input": "../js-lib/yom/src/core/core-pkg.js",
			"output": "static/js/lib/yom/core-pkg.js"
		},
		{
			"input": "../js-lib/yom/src/ext/ext-pkg.js",
			"output": "static/js/lib/yom/ext-pkg.js"
		},
		{
			"input": "../js-lib/yom/src/dragdrop/dragdrop-pkg.js",
			"output": "static/js/lib/yom/dragdrop-pkg.js"
		},
		{
			"input": "../js-lib/yom/widget/src/dialog/dialog.js",
			"output": "static/js/lib/yom/widget/dialog.js"
